{
  "name": "tailwindcss-mangle",
  "type": "module",
  "version": "4.0.0",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "turbo run build --filter=./packages/*",
    "dev": "pnpm -r run dev",
    "test": "turbo run test --filter=./packages/*",
    "test:dev": "vitest",
    "build:docs": "turbo run build --filter=./website",
    "lint": "eslint packages/**/*.ts --fix",
    "preinstall": "npx only-allow pnpm",
    "sync": "cnpm sync @tailwindcss-mangle/shared @tailwindcss-mangle/config tailwindcss-patch unplugin-tailwindcss-mangle @tailwindcss-mangle/core",
    "publish-packages": "pnpm run build && pnpm run test && changeset version && changeset publish",
    "-------": "-----------",
    "release": "changeset",
    "pr": "changeset pre enter alpha",
    "pr:beta": "changeset pre enter beta",
    "pr:rc": "changeset pre enter rc",
    "pr:exit": "changeset pre exit",
    "cv": "changeset version ",
    "--------": "----------------",
    "_postinstall": "tw-patch install",
    "script:init": "monorepo init",
    "script:sync": "monorepo sync",
    "script:clean": "monorepo clean",
    "script:mirror": "monorepo mirror",
    "commitlint": "commitlint --edit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/prompt-cli": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@icebreakers/eslint-config": "^1.4.4",
    "@icebreakers/monorepo": "^2.0.1",
    "@icebreakers/stylelint-config": "^1.1.4",
    "@rollup/pluginutils": "^5.3.0",
    "@tailwindcss-mangle/core": "workspace:*",
    "@tailwindcss-mangle/shared": "workspace:*",
    "@tsconfig/recommended": "^1.0.10",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__helper-plugin-utils": "^7.10.3",
    "@types/babel__traverse": "^7.28.0",
    "@types/fs-extra": "^11.0.4",
    "@types/get-value": "^3.0.5",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/klaw": "^3.0.7",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^24.5.2",
    "@types/resolve": "^1.20.6",
    "@types/semver": "^7.7.1",
    "@types/set-value": "^4.0.3",
    "@vitest/coverage-v8": "~3.2.4",
    "astro": "^5.14.1",
    "ci-info": "^4.3.0",
    "comment-json": "^4.3.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "dedent": "^1.7.0",
    "defu": "^6.1.4",
    "del": "^8.0.1",
    "dotenv": "^17.2.2",
    "es-toolkit": "^1.39.10",
    "eslint": "^9.36.0",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fdir": "^6.5.0",
    "fs-extra": "^11.3.2",
    "get-value": "^4.0.1",
    "html-minifier-terser": "^7.2.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "klaw": "^4.1.0",
    "lint-staged": "^16.2.3",
    "local-pkg": "^1.1.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.8",
    "mini-css-extract-plugin": "^2.9.4",
    "normalize-newline": "^5.0.0",
    "only-allow": "^1.2.1",
    "openai": "^5.23.1",
    "pathe": "^2.0.3",
    "picomatch": "^4.0.3",
    "pkg-types": "^2.3.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "postcss7": "npm:postcss@7",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.3",
    "set-value": "^4.1.0",
    "tailwindcss": "catalog:tailwindcss3",
    "tailwindcss-patch": "workspace:*",
    "tailwindcss2": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "turbo": "^2.5.8",
    "type-fest": "^5.0.1",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "unplugin": "^2.3.10",
    "unplugin-tailwindcss-mangle": "workspace:*",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "~3.2.4",
    "webpack": "^5.101.3",
    "webpack-build-utils": "^0.0.7",
    "yaml": "^2.8.1"
  }
}
