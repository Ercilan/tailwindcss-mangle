{
  "name": "tailwindcss-mangle",
  "type": "module",
  "version": "2.2.2",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@9.1.4",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "dev": "pnpm -r run dev",
    "test": "vitest run",
    "test:dev": "vitest",
    "lint": "eslint packages/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm",
    "sync": "cnpm sync @tailwindcss-mangle/shared @tailwindcss-mangle/config tailwindcss-patch unplugin-tailwindcss-mangle @tailwindcss-mangle/core",
    "publish-packages": "pnpm run test && changeset version && changeset publish",
    "release": "changeset",
    "_postinstall": "tw-patch install"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@icebreakers/eslint-config": "^0.3.12",
    "@rollup/pluginutils": "^5.1.0",
    "@tailwindcss-mangle/core": "workspace:*",
    "@tailwindcss-mangle/shared": "workspace:*",
    "@tsconfig/recommended": "^1.0.7",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__helper-plugin-utils": "^7.10.3",
    "@types/babel__traverse": "^7.20.6",
    "@types/fs-extra": "^11.0.4",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.11",
    "@types/resolve": "^1.20.6",
    "@types/semver": "^7.5.8",
    "@vitest/coverage-v8": "^2.0.3",
    "astro": "^4.12.1",
    "bumpp": "^9.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dedent": "^1.5.3",
    "defu": "^6.1.4",
    "del": "^7.1.0",
    "eslint": "^9.7.0",
    "fs-extra": "^11.2.0",
    "html-minifier-terser": "^7.2.0",
    "html-webpack-plugin": "^5.6.0",
    "local-pkg": "^0.5.0",
    "lodash-es": "^4.17.21",
    "mini-css-extract-plugin": "^2.9.0",
    "normalize-newline": "^4.1.0",
    "only-allow": "^1.2.1",
    "pathe": "^1.1.2",
    "pkg-types": "^1.1.3",
    "postcss": "^8.4.39",
    "postcss-loader": "^8.1.1",
    "postcss7": "npm:postcss@7",
    "prettier": "^3.3.3",
    "rollup": "^4.18.1",
    "tailwindcss": "^3.4.6",
    "tailwindcss-patch": "workspace:*",
    "tailwindcss2": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsup": "^8.2.0",
    "tsx": "^4.16.2",
    "turbo": "^2.0.7",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "unplugin": "^1.11.0",
    "unplugin-tailwindcss-mangle": "workspace:*",
    "vite": "^5.3.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.3",
    "webpack": "^5.93.0",
    "webpack-build-utils": "^0.0.7"
  }
}
