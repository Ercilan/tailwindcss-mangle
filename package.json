{
  "name": "tailwindcss-mangle",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@icebreakers/eslint-config-ts": "^1.0.4",
    "@icebreakers/rollup": "^0.2.2",
    "@icebreakers/tsconfig": "^0.0.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "turbo": "^1.8.8",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.14.1"
}