{
  "name": "tailwindcss-mangle",
  "type": "module",
  "version": "2.2.2",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "dev": "pnpm -r run dev",
    "test": "vitest run",
    "test:dev": "vitest",
    "lint": "eslint packages/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm",
    "sync": "cnpm sync @tailwindcss-mangle/shared @tailwindcss-mangle/config tailwindcss-patch unplugin-tailwindcss-mangle @tailwindcss-mangle/core",
    "publish-packages": "pnpm run test && changeset version && changeset publish",
    "-------": "-----------",
    "release": "changeset",
    "pr": "changeset pre enter alpha",
    "pr:beta": "changeset pre enter beta",
    "pr:rc": "changeset pre enter rc",
    "pr:exit": "changeset pre exit",
    "cv": "changeset version ",
    "--------": "----------------",
    "_postinstall": "tw-patch install",
    "script:init": "monorepo init",
    "script:sync": "monorepo sync",
    "script:clean": "monorepo clean",
    "script:mirror": "monorepo mirror"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@icebreakers/eslint-config": "^0.6.1",
    "@icebreakers/monorepo": "^0.6.11",
    "@icebreakers/stylelint-config": "^0.1.2",
    "@rollup/pluginutils": "^5.1.2",
    "@tailwindcss-mangle/core": "workspace:*",
    "@tailwindcss-mangle/shared": "workspace:*",
    "@tsconfig/recommended": "^1.0.7",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__helper-plugin-utils": "^7.10.3",
    "@types/babel__traverse": "^7.20.6",
    "@types/fs-extra": "^11.0.4",
    "@types/get-value": "^3.0.5",
    "@types/git-url-parse": "^9.0.3",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/klaw": "^3.0.6",
    "@types/lint-staged": "^13.3.0",
    "@types/lodash": "^4.17.12",
    "@types/lodash-es": "^4.17.12",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.7.7",
    "@types/resolve": "^1.20.6",
    "@types/semver": "^7.5.8",
    "@types/set-value": "^4.0.3",
    "@vitest/coverage-v8": "~2.1.3",
    "astro": "^4.16.7",
    "ci-info": "^4.0.0",
    "comment-json": "^4.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dedent": "^1.5.3",
    "defu": "^6.1.4",
    "del": "^8.0.0",
    "eslint": "^9.13.0",
    "execa": "^9.4.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "get-value": "^3.0.1",
    "html-minifier-terser": "^7.2.0",
    "html-webpack-plugin": "^5.6.2",
    "husky": "^9.1.6",
    "klaw": "^4.1.0",
    "lint-staged": "^15.2.10",
    "local-pkg": "^0.5.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.8",
    "mini-css-extract-plugin": "^2.9.1",
    "normalize-newline": "^4.1.0",
    "only-allow": "^1.2.1",
    "pathe": "^1.1.2",
    "pkg-types": "^1.2.1",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "postcss7": "npm:postcss@7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "set-value": "^4.1.0",
    "tailwindcss": "^3.4.14",
    "tailwindcss-patch": "workspace:*",
    "tailwindcss2": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "tslib": "^2.8.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "turbo": "^2.2.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "unplugin": "^1.14.1",
    "unplugin-tailwindcss-mangle": "workspace:*",
    "vite": "^5.4.9",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "~2.1.3",
    "webpack": "^5.95.0",
    "webpack-build-utils": "^0.0.7",
    "yaml": "^2.6.0"
  }
}
