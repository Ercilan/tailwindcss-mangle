{
  "name": "tailwindcss-mangle",
  "type": "module",
  "version": "2.2.2",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@9.1.4",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "dev": "pnpm -r run dev",
    "test": "vitest run",
    "test:dev": "vitest",
    "lint": "eslint packages/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm",
    "sync": "cnpm sync @tailwindcss-mangle/shared @tailwindcss-mangle/config tailwindcss-patch unplugin-tailwindcss-mangle @tailwindcss-mangle/core",
    "publish-packages": "pnpm run test && changeset version && changeset publish",
    "release": "changeset",
    "_postinstall": "tw-patch install"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@icebreakers/eslint-config": "^0.3.12",
    "@rollup/pluginutils": "^5.1.0",
    "@tailwindcss-mangle/core": "workspace:*",
    "@tailwindcss-mangle/shared": "workspace:*",
    "@tsconfig/recommended": "^1.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.11",
    "@vitest/coverage-v8": "^2.0.3",
    "bumpp": "^9.4.1",
    "cross-env": "^7.0.3",
    "dedent": "^1.5.3",
    "defu": "^6.1.4",
    "del": "^7.1.0",
    "eslint": "^9.7.0",
    "fs-extra": "^11.2.0",
    "html-minifier-terser": "^7.2.0",
    "local-pkg": "^0.5.0",
    "lodash-es": "^4.17.21",
    "only-allow": "^1.2.1",
    "pathe": "^1.1.2",
    "prettier": "^3.3.3",
    "rollup": "^4.18.1",
    "tailwindcss-patch": "workspace:*",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsup": "^8.1.2",
    "turbo": "^2.0.7",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "unplugin": "^1.11.0",
    "unplugin-tailwindcss-mangle": "workspace:*",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.3"
  }
}
